# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key-here

# Firestore Configuration
FIRESTORE_PROJECT_ID=agentnav-dev
FIRESTORE_DATABASE_ID=default

# Backend Configuration
BACKEND_URL=http://agentnav-backend:8080
ENVIRONMENT=development

# Frontend Configuration
VITE_API_URL=http://localhost:8080
# Frontend should call the backend API endpoint instead of using API keys directly
# This is only for legacy/compatibility - remove before production deployment

# ==============================================================================
# Supabase Configuration (for Gen AI Prompt Management App)
# ==============================================================================
# Supabase project URL (e.g., https://xxxxxxxxxxxxx.supabase.co)
SUPABASE_URL=https://your-project-id.supabase.co

# Supabase anonymous key (public key for client-side usage)
SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Supabase service role key (private key for server-side usage - keep secure!)
SUPABASE_SERVICE_KEY=your-supabase-service-key-here

# For Next.js app compatibility (if using Next.js for Prompt Management App)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here
